 AS R ON (R.id = T1.id_cliente) '\\r\\n                                                ],\\r\\n                                                fieldSearch : [\\r\\n                                                    'PT.nombre',\\r\\n                                                    'R.razon_social',\\r\\n                                                    'T1.peso'\\r\\n                                                ],\\r\\n                                                sqlEmpresa : \\\"true\\\",\\r\\n                                                fieldFechas : \\\"T1.fecha_venta\\\"                    \\r\\n                                            }                                                                                     \\r\\n                                        },                                        \\r\\n                                    ]}                        \\r\\n                    />          \\r\\n                </div>\\t\\r\\n                <div id=\\\"reportContainer\\\" className=\\\"reportContainer\\\" data-role=\\\"reportTbar\\\" os=\\\"windows\\\" >\\r\\n                    <ReportContainer optionMenu={this.state.optionMenu} />\\r\\n                </div>\\r\\n            </div>\\r\\n  \\t  \\t);\\r\\n  \\t}\\r\\n}\\r\\n\\r\\nexport default Reports\\r\\n\",\"/**\\r\\n* CLASS WelcomePage\\r\\n*\\r\\n* Contiene la pantalla de bienvenida del software\\r\\n* esta se carga en el contenedor de visualizacion \\r\\n*\\r\\n* @author Hector Morales <warrior1987@gmail.com>\\r\\n*/\\r\\n\\r\\n\\r\\nimport React, { Component } from 'react';\\r\\nimport './desktop.css';\\r\\nimport logo_fondo from '../../images/logo_fondo.png?v1.0';\\r\\n\\r\\nclass WelcomePage extends Component {\\t\\r\\n  \\trender() {\\r\\n  \\t  \\treturn (\\r\\n  \\t  \\t\\t<div className=\\\"WelcomeFondo\\\" style={{backgroundImage: `url(${ logo_fondo })`}}>  \\t  \\t\\t\\t\\t\\t  \\r\\n  \\t  \\t\\t\\t\\r\\n  \\t  \\t\\t</div>\\t\\t    \\t\\t\\r\\n\\t    );\